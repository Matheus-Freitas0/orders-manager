{
  "name": "orders-manager",
  "version": "1.0.0",
  "description": "A Node.js application for managing orders",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/main",
    "start:dev": "set APP_ENV=dev && nodemon",
    "debug": "tsc && node --inspect-brk dist/src/main.js",
    "start:pm2": "npm run build && pm2 start pm2.config.js",
    "start:pm2Docker": "npm run build && pm2-docker start pm2.config.js",
    "test": "export APP_ENV=dev && npm run build && node dist/src/test.js",
    "jest": "npm run build && jest dist/test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Matheus-Freitas0/orders-manager.git"
  },
  "author": "Matheus Freitas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Matheus-Freitas0/orders-manager/issues"
  },
  "homepage": "https://github.com/Matheus-Freitas0/orders-manager#readme",
  "keywords": [
    "nodejs",
    "typescript",
    "orders",
    "management"
  ],
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/redis": "^4.0.10",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/amqplib": "^0.10.5",
    "amqplib": "^0.10.4",
    "axios": "^1.3.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.10.3",
    "rabbitmq-client": "^5.0.0",
    "redis": "^4.7.0"
  }
}
